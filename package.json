    "http-server": "^0.12.3",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "shelljs": "^0.8.4",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/blessed": "^0.1.17",
    "@types/cookie": "^0.4.0",
    "@types/http-proxy": "^1.17.4",
    "@types/node-fetch": "^2.5.7",
