{
  "name": "angular-universal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run angular-universal:serve-ssr",
    "serve:ssr": "node dist/angular-universal/server/main.js",
    "build:ssr": "ng build && ng run angular-universal:server",
    "prerender": "ng run angular-universal:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nguniversal/express-engine": "^13.1.1",
    "express": "^4.15.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.11.1",
    "typescript": "~4.6.2"
  }
}
